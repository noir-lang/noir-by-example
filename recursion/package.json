{
  "name": "noir-starter",
  "version": "1.0.0",
  "description": "A template repository to get started with writing zero knowledge programs with Noir.",
  "type": "module",
  "scripts": {
    "dev": "yarn deploy:localhost && next",
    "test": "hardhat compile && NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only' hardhat test",
    "deploy:localhost": "hardhat compile && NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only' hardhat run --network localhost scripts/deploy.ts"
  },
  "dependencies": {
    "@noir-lang/backend_barretenberg": "0.19.4",
    "@noir-lang/noir_js": "0.19.4",
    "@noir-lang/noir_wasm": "0.19.4",
    "@noir-lang/source-resolver": "0.19.4",
    "@noir-lang/types": "0.19.4",
    "@nomicfoundation/hardhat-network-helpers": "1.0.1",
    "@nomicfoundation/hardhat-toolbox-viem": "1.0.0",
    "@nomicfoundation/hardhat-viem": "1.0.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-verify": "1.0.1",
    "@types/chai": "^4.2.0",
    "axios": "^1.5.1",
    "chai": "^4.2.0",
    "dotenv": "^16.0.3",
    "hardhat": "^2.19.2",
    "hardhat-gas-reporter": "^1.0.8",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.1",
    "solidity-coverage": "^0.8.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "viem": "1.15.1",
    "wagmi": "^1.4.3"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "typescript": "^4.9.3"
  }
}
